{
  "name": "duckuments",
  "displayName": "duckuments",
  "description": "AI Assistant for Generate Code Documentation",
  "version": "0.1.0",
  "publisher": "Hirmaan Rashidi (duckuments)",
  "icon": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/duckuments/vscode-duckuments-chat.git"
  },
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Programming Languages",
    "Machine Learning",
    "Other"
  ],
  "keywords": [
    "documentation",
    "ai",
    "llm",
    "ollama",
    "copilot",
    "generator",
    "markdown"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onView:duckumentsChatView"
  ],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "duckumentsSideBar",
          "title": "duckuments",
          "icon": "./media/icon.svg"
        }
      ]
    },
    "views": {
      "duckumentsSideBar":[
        {
          "id": "duckumentsChatView",
          "name": "Chat",
          "icon": "./media/icon.svg"
        }
      ]
    },
    "commands": [{
      "command": "duckuments.run",
      "title": "Run duckuments"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.36.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
